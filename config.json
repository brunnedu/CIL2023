{
    "model":
    {
        "UNet": {
            "backbone": {
                "Resnet18": {}
            },
            "up_block_ctor": {
                "UpBlock": {
                    "up_mode": "upsample"
                }
            }
        }
    },
    "criterion": 
    {
        "FocalLoss": {
            "alpha": 0.25, 
            "gamma": 2.0, 
            "bce_reduction": "none"
        }
    },
    "optimizer":
    {
        "Adam": {
            "parameters": "all",
            "lr": 0.001
        }   
    }
}